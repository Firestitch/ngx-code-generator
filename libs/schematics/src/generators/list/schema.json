{
  "$schema": "http://json-schema.org/schema",
  "id": "SchemanticsForList",
  "title": "List Schema",
  "type": "object",
  "properties": {
    "path": {
      "type": "string",
      "format": "path",
      "description": "Path to the module desctination"
    },
    "module": {
      "type": "string",
      "description": "Name of module file"
    },
    "type": {
      "type": "string",
      "description": "Component or View type"
    },
    "name": {
      "type": "string",
      "description": "Name of component for create"
    },
    "service": {
      "type": "string",
      "description": "Name of service"
    },
    "servicePath": {
      "type": "string",
      "format": "path",
      "description": "Path for service"
    },
    "singleModel": {
      "type": "string",
      "description": ""
    },
    "pluralModel": {
      "type": "string",
      "description": ""
    },
    "routableComponent": {
      "type": "boolean",
      "description": "If true then move component to /views"
    },
    "titledComponent": {
      "type": "boolean",
      "description": "If true then add the setTitle() function"
    }
  }
}
